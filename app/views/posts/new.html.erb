<div class="contents row">
  <div class="container">
    <h3>みんなに情報を共有しよう</h3>
    <%= form_with(model: @post, local: true) do |post| %>
      <%= post.text_area :text, placeholder: "text", rows: "10" %>
      <%= post.file_field :image, id:"image" %>
      <%= post.submit "投稿" %>
    <% end %>
  </div>
</div>