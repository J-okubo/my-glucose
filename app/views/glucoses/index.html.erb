<div class= "glucose_nav">
 <p>血糖値管理表<i class="fa-solid fa-syringe"></i></p>
</div>
<table>
  <thead>
    <tr>
      <th>日付</th>
      <th>朝食前</th>
      <th>朝食後</th>
      <th>昼食前</th>
      <th>昼食後</th>
      <th>夕食前</th>
      <th>夕食後</th>
      <th>備考欄</th>
    </tr>
  </thead>
  <tbody>
    <% @glucoses.each do |glucose| %>
      <tr>
        <td><%= glucose.date %></td>
        <td><%= glucose.before_breakfast_glucose %></td>
        <td><%= glucose.after_breakfast_glucose %></td>
        <td><%= glucose.before_lunch_glucose %></td>
        <td><%= glucose.after_lunch_glucose %></td>
        <td><%= glucose.before_dinner_glucose %></td>
        <td><%= glucose.after_dinner_glucose %></td>
        <td><%= glucose.remarks_section %></td>
      </tr>
    <% end %>
  </tbody>
</table>